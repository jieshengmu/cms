<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.itsource.mapper.ArticleTypeMapper">
<!-- findTypeById(); 根据id查询文章类型-->
<select id="findTypeById" resultType="ArticleType">
select * from t_article_type  where id = #{id}
</select>
<!-- List<ArticleType> findTypeAll(); 查询所有文章类型-->
<select id="findTypeAll" resultType="ArticleType">
select * from t_article_type
</select>


</mapper>